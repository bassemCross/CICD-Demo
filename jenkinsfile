pipeline { 
    agent any  
    stages { 
        stage('Build') { 
            steps { 
               echo 'Build.'
               sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Image') { 
            steps { 
               echo 'Image.'
               sh 'docker build docker build -t NewImage .'
            }
        }
    }
}